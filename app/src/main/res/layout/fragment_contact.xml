<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-21dp">

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="269dp"
        android:layout_height="34dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.11">

        <TextView
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="You can contact us any time. "
            android:textColor="#000000"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="71dp"
            tools:layout_editor_absoluteY="77dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="196dp"
        android:layout_height="40dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.175">

        <TextView
            android:id="@+id/text3"
            android:layout_width="191dp"
            android:layout_height="31dp"
            android:text="We are here for you !"
            android:textColor="#000000"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="107dp"
            tools:layout_editor_absoluteY="121dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="70dp"
        android:layout_height="48dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.326">

        <ImageView
            android:id="@+id/mail"
            android:layout_width="match_parent"
            android:layout_height="38dp"
            app:srcCompat="@drawable/ic_baseline_email_24"
            tools:layout_editor_absoluteX="170dp"
            tools:layout_editor_absoluteY="223dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="61dp"
        android:layout_height="47dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.706">

        <ImageView
            android:id="@+id/phoneImage"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ic_baseline_call_24"
            tools:layout_editor_absoluteX="176dp"
            tools:layout_editor_absoluteY="483dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout17"
        android:layout_width="58dp"
        android:layout_height="47dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.527">

        <ImageView
            android:id="@+id/locationImage"
            android:layout_width="24dp"
            android:layout_height="38dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/location_ppin"
            tools:layout_editor_absoluteX="175dp"
            tools:layout_editor_absoluteY="361dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="321dp"
        android:layout_height="54dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8">

        <Button
            android:id="@+id/tel1"
            android:layout_width="149dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#FFFFFF"
            android:padding="0dp"
            android:paddingStart="0dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingEnd="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:phoneNumber="true"
            android:text="+4531000000"
            android:textColor="@android:color/black"
            app:iconPadding="0dp"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="542dp" />

        <Space
            android:layout_width="24dp"
            android:layout_height="46dp"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="194dp"
            tools:layout_editor_absoluteY="542dp" />

        <Button
            android:id="@+id/tel2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="#FFFFFF"
            android:includeFontPadding="false"
            android:padding="0dp"
            android:paddingStart="0dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingEnd="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:text="+4571000000"
            android:textColor="#000000"
            app:iconPadding="0dp"
            tools:layout_editor_absoluteX="241dp"
            tools:layout_editor_absoluteY="543dp" />

    </LinearLayout>

    <Button
        android:id="@+id/tel3"
        android:layout_width="153dp"
        android:layout_height="48dp"
        android:backgroundTint="#FFFFFF"
        android:text="+46000000"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.874" />

    <Button
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        android:backgroundTint="#FFFFFF"
        android:text="info@hs-services.dk"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.408"
        app:layout_marginBaseline="0dp" />

    <Button
        android:id="@+id/location"
        android:layout_width="330dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/white"
        android:text="Stationsvej 31, 9400 NÃ¸rresundby"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.59" />

</androidx.constraintlayout.widget.ConstraintLayout>